%h1 New feature

- form_for(@feature) do |f|
  = f.error_messages
  %p
    = f.label :title
    %br
    = f.text_field :title
  %p
    = f.label :why
    %br
    = f.text_field :why
  %p
    = f.label :who
    %br
    = f.text_field :who
  %p
    = f.label :what
    %br
    = f.text_field :what
  %p= f.submit "Create"
= link_to 'Back', features_path